const countryRoutes = require ("express").Router();

const {getAllCountries, getCountryByCode, createCountry, deleteCountry} = require("../controllers/countryControllers");

countryRoutes.get("/", getAllCountries);
countryRoutes.get("/:code", getCountryByCode);
countryRoutes.get("/", createCountry);
countryRoutes.get("/:code", deleteCountry);

module.exports = countryRoutes;